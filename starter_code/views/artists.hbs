<div class="container">
  <div class="row">
  {{#each this}}
    <div class="col-lg-4 artist-container">

      <img src="{{this.images.url}}" alt="" class="artist-img">

      <div class="artist-info-container">
        <p>{{this.name}}</p>
        <a href="/albums/{{this.id}}">Albums</a>
      </div>
    </div>
  {{/each}}
  </div>
</div>